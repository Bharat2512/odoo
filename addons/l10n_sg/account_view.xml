<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

    <record model="ir.ui.view" id="view_tax_code_form_inherit">
        <field name="name">account.tax.code.form.inherit</field>
        <field name="model">account.tax.code</field>
        <field name="inherit_id" ref="account.view_tax_code_form"/>
        <field name="priority" eval="20"/>
        <field name="arch" type="xml">

            <xpath expr="//group[@string='Reporting Configuration']/field[@name='notprintable']" position="before">
                <field name="printable"/>
            </xpath>

            <xpath expr="//group[@string='Reporting Configuration']/field[@name='notprintable']" position="after">
                <field name="sequence"/>
            </xpath>

        </field>
    </record>
    
    </data>
</openerp>
