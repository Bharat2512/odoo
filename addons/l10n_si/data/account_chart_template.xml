<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- Chart Template -->
    <record id="transfer_account_id" model="account.account.template">
        <field name="name">prenos računa</field>
        <field name="code">1111</field>
        <field name="reconcile" eval="True"/>
        <field name="user_type_id" ref="account.data_account_type_current_assets"/>
    </record>
    <record id="gd_chart" model="account.chart.template">
        <field name="name">Kontni načrt za gospodarske družbe</field>
        <field name="bank_account_code_prefix">550</field>
        <field name="cash_account_code_prefix">551</field>
        <field name="code_digits">6</field>
        <field name="currency_id" ref="base.EUR"/>
        <field name="transfer_account_id" ref="transfer_account_id"/>
    </record>
    <record id="transfer_account_id" model="account.account.template">
        <field name="chart_template_id" ref="gd_chart"/>
    </record>

</odoo>
