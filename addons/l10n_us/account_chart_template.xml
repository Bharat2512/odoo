<?xml version="1.0" encoding="UTF-8"?>
<odoo>

        <record id="transfer_account" model="account.account.template">
            <field name="name">Transfer Account</field>
            <field name="code">101</field>
            <field name="reconcile" eval="True"/>
            <field name="user_type_id" ref="account.data_account_type_current_assets"/>
        </record>
        <record id="account_chart_template_basic" model="account.chart.template">
            <field name="name">Basic Chart of Account</field>
            <field name="visible" eval="False"/>
            <field name="complete_tax_set" eval="False"/>
            <field name="use_anglo_saxon" eval="True"/>
            <field name="bank_account_code_prefix">1101</field>
            <field name="cash_account_code_prefix">1102</field>
            <field name="currency_id" ref="base.USD"/>
            <field name="code_digits">6</field>
            <field name="transfer_account_id" ref="transfer_account"/>
        </record>
        <record id="transfer_account" model="account.account.template">
            <field name="chart_template_id" ref="account_chart_template_basic"/>
        </record>

