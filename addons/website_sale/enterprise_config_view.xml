<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_website_sale_enterprise_config_settings" model="ir.ui.view">
        <field name="name">website sales enterprise settings</field>
        <field name="model">website.config.settings</field>
        <field name="inherit_id" ref="website_sale.website_config_settings_view_form"/>
        <field name="arch" type="xml">
            <field name="cdn_filters" position="after">
                <field name="shipping_integration" class="oe_inline" widget="upgrade_radio"/>
                <field name="ebay_connector" class="oe_inline" widget="upgrade_radio"/>
                <field name="amazon_connector" class="oe_inline" widget="upgrade_radio"/>
                <field name="coupons" class="oe_inline" widget="upgrade_radio"/>
            </field>
        </field>
    </record>
</odoo>
